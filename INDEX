<!-- WHATSAPP + VITRINE PROFISSIONAL -->
<script>
document.addEventListener("DOMContentLoaded", function () {

  const phone = "5511949792703";

  /* REMOVE CARRINHO E LOGIN */
  document.querySelectorAll(
    '[href*="comprar"], [href*="cart"], [href*="login"], [href*="account"]'
  ).forEach(el => el.remove());

  /* TRANSFORMA PRODUTOS EM LINK WHATSAPP */
  document.querySelectorAll(".js-item-product").forEach(item => {

    const name = item.querySelector(".js-item-name")?.innerText.trim();
    const price = item.querySelector(".js-price-display")?.innerText.trim();

    if (!name) return;

    const msg =
`Ol! 

Tenho interesse nesta camisa:

 Produto: ${name}
 Valor: ${price || "Consultar"}

Pode me informar disponibilidade e tamanhos?`;

    const link = "https://wa.me/" + phone + "?text=" + encodeURIComponent(msg);

    item.style.cursor = "pointer";

    item.onclick = e => {
      e.preventDefault();
      window.open(link, "_blank");
    };

    item.querySelectorAll("a").forEach(a => {
      a.removeAttribute("href");
    });

  });


  /* BOTO FLUTUANTE WHATSAPP */
  const btn = document.createElement("a");

  btn.href = "https://wa.me/" + phone;
  btn.target = "_blank";

  btn.innerHTML = " Fale no WhatsApp";

  btn.style = `
    position: fixed;
    bottom: 20px;
    right: 20px;
    background: #25D366;
    color: white;
    padding: 14px 18px;
    border-radius: 50px;
    font-weight: bold;
    font-size: 14px;
    text-decoration: none;
    box-shadow: 0 4px 10px rgba(0,0,0,.3);
    z-index: 9999;
  `;

  document.body.appendChild(btn);

});
</script>

</body>
</html>
